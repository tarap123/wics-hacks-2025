{
    "manifest_version": 3,
    "name": "Saddle Up & Work",
    "version": "1.0",
    "description": "A cute western-themed productivity extension with tab management and Pomodoro focus timer!",
    "permissions": ["tabs", "storage", "notifications", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon16.png",
    "128": "assets/icon48.png"
  }
    }